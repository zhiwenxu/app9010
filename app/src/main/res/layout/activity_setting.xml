<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include layout="@layout/common_title_right_txt"/>
<com.example.view.commonview.UpDownScrollView 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:scrollbars="none"
    android:background="@color/app_background_color">
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout 
            android:id="@+id/question_btn"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_marginTop="15dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="常见问题"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/app_divider_color"/>
        <RelativeLayout 
            android:id="@+id/yhtk_btn"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="用户条款"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/app_divider_color"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="给我打分"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/aboutus_btn"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_marginTop="15dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="关于我们"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/app_divider_color"/>
        <RelativeLayout 
            android:id="@+id/feedback"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="意见反馈"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_marginTop="15dip"
            android:background="@color/white">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="消息通知"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/app_divider_color"/>
        <RelativeLayout 
            android:id="@+id/clear_btn"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/title_bar_tv_color"
                android:text="清除缓存"/>
            <ImageView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/yjt"/>
        </RelativeLayout>
        
        <RelativeLayout 
            android:id="@+id/exit_btn"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_marginTop="15dip"
            android:clickable="true"
            android:background="@drawable/layout_btn_selector">
            <TextView 
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="14sp"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:textColor="@color/bottom_bar_tv_color_red"
                android:text="退出登陆"/>
        </RelativeLayout>
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/erweima"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="15dip"/>
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginTop="15dip"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/title_bar_tv_color"
            android:text="成都玖零壹零智慧超市"/>
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginTop="3dip"
            android:layout_marginBottom="5dip"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/title_bar_tv_color"
            android:text="CopyRight 2016-2020"/>
    </LinearLayout>
</com.example.view.commonview.UpDownScrollView>
</LinearLayout>
